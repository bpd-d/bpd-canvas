{
  "name": "bpd-canvas",
  "version": "1.0.0",
  "description": "Simple library for canvas opertations",
  "main": "index.ts",
  "scripts": {
    "build": "webpack --config webpack.production.js",
    "build:minify": "webpack --config webpack.build.minify.js",
    "test": "karma start karma.config.js",
    "build:esm": "tsc -p tsconfig.esm.json",
    "prod": "npm run build && npm run build:esm && npm run build:minify",
    "dev": "webpack-dev-server --config webpack.development.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bpd-d/bpd-canvas.git"
  },
  "keywords": [
    "canvas",
    "bpd"
  ],
  "author": "BPDev",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/bpd-d/bpd-canvas/issues"
  },
  "homepage": "https://github.com/bpd-d/bpd-canvas#readme",
  "devDependencies": {
    "@types/jasmine": "^3.5.12",
    "clean-webpack-plugin": "^3.0.0",
    "jasmine": "^3.6.1",
    "karma-typescript": "^5.0.3",
    "serialize-javascript": ">=2.1.1",
    "terser-webpack-plugin": "^3.1.0",
    "ts-loader": "^6.2.2",
    "typescript": "^3.9.7",
    "typescript-transform-paths": "^1.1.10",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2",
    "webpack-karma-jasmine": "^4.0.0",
    "webpack-merge": "^4.2.2"
  }
}
